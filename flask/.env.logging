# Logging Configuration Environment Variables
# Copy these to your .env file or set as environment variables

# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Verbose logging: shows timestamps and detailed formatting
LOG_VERBOSE=false

# Cache debug: enables detailed cache operation logging
CACHE_DEBUG=false

# Example configurations:
# Production: LOG_LEVEL=WARNING, LOG_VERBOSE=false, CACHE_DEBUG=false
# Development: LOG_LEVEL=INFO, LOG_VERBOSE=true, CACHE_DEBUG=false
# Debug: LOG_LEVEL=DEBUG, LOG_VERBOSE=true, CACHE_DEBUG=true